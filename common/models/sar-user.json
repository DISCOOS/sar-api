{
  "name": "SARUser",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "remoting": {
      "sharedMethods": {
        "*": false,
        "login": true,
        "persons": true
      }
    }
  },
  "hidden": [
    "kovaId"
  ],
  "properties": {
    "kovaId": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "isAvailable": {
      "type": "boolean",
      "required": true
    },
    "isTrackable": {
      "type": "boolean",
      "required": true
    },
    "isAdmin": {
      "type": "boolean",
      "required": true
    },
    "deviceToken": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "missions": {
      "type": "hasMany",
      "model": "Mission",
      "foreignKey": "creator"
    },
    "alarms": {
      "type": "hasMany",
      "model": "Alarm",
      "foreignKey": "alarmId",
      "through": "AlarmUser"
    },
    "expenses": {
      "type": "hasMany",
      "model": "Expence",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}